{
  "redirects": [
    {
      "source": "/privacy",
      "destination": "/privacy-policy",
      "permanent": true
    },
    {
      "source": "/privacy.html",
      "destination": "/privacy-policy",
      "permanent": true
    },
    {
      "source": "/terms",
      "destination": "/terms-of-service",
      "permanent": true
    },
    {
      "source": "/terms.html",
      "destination": "/terms-of-service",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/terms-of-service",
      "destination": "/terms.html"
    },
    {
      "source": "/privacy-policy",
      "destination": "/privacy.html"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
